{
  "name": "@aurory/dnajs",
  "version": "0.7.10",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aurory-Game/dna.git"
  },
  "bugs": {
    "url": "https://github.com/Aurory-Game/dna/issues"
  },
  "scripts": {
    "sp": "ts-node $1",
    "test": "ts-mocha -p ./tsconfig.json -t 2500000 --exit tests/**/*.spec.ts",
    "test:dna": "ts-mocha -p ./tsconfig.json -t 2500000 --exit tests/dna.spec.ts",
    "test:distribution": "ts-mocha -p ./tsconfig.json -t 2500000 --exit tests/distribution.spec.ts",
    "build": "tsc -p tsconfig.build.json && cp -r src/deps lib/",
    "prepare": "cd .. && husky install && cd ts && yarn build",
    "prepublishOnly": "yarn test",
    "lint:fix": "eslint --fix --ext .ts,.json . && tsc --pretty",
    "lint": "eslint . --ext .ts,.tsx,.json && tsc --pretty",
    "i18n:sync": "ts-node bin/scripts/sync.ts"
  },
  "author": "Aurory",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/eslint": "^8.4.3",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/expect": "^24.3.0",
    "@types/google-spreadsheet": "3.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.2",
    "@types/prettier": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "sideEffects": false,
  "type": "commonjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@types/randombytes": "^2.0.0",
    "camel-case": "^4.1.2",
    "dotenv": "^16.3.1",
    "google-spreadsheet": "^3.3.0",
    "randombytes": "^2.1.0",
    "snake-case": "^3.0.4"
  }
}
